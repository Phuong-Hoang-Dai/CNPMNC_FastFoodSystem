                @Html.DisplayFor(modelItem => item.Staff.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FFSDeliveryRecievedNoteId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FFSDeliveryRecievedNoteId">Details</a> |
